{
    "devel": true,
    "node": true,
    "esnext": true,

    "bitwise": true,
    "curly": true,
    "eqeqeq": true,
    "eqnull": true,
    "forin": false,
    "immed": true,
    "indent": 4,
    "newcap": true,
    "noarg": true,
